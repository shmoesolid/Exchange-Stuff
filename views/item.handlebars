
<p>display form for creating, editing, or removing</p>

{{#if item.error}}
    <p>Error: {{item.error}}</p>
{{else if item.id}}
    <p>{{item.id}}</p>
    <p>{{item.title}}</p>
    <p>{{item.description}}</p>
    <p>{{item.minValue}}</p>
    <p>{{item.maxValue}}</p>
    <p>{{item.userId}}</p>
{{else}}
    <p>no item</p>
{{/if}}

<script type="text/javascript" src="js/item.js"></script>
